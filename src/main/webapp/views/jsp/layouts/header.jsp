<%@page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<header class="header">
  <div class="container-fluid">
    <div class="row header-content">
      <div class="col-8">
        <div class="logo">
          <i class="bx bx-football"></i><span class="log-name">Soccer Booking</span>
        </div>
      </div>
      <div class="col-4">
        <div class="menu d-block d-sm-none">
          <i class="bx bx-menu"></i>
        </div>
        <div
          id="log-out"
          data-toggle="tooltip"
          data-placement="bottom"
          title="Log out"
        >
          <a href="/logout"
            ><i class="bx bx-log-out d-none d-sm-inline-block"></i
          ></a>
        </div>
      </div>
    </div>
  </div>
</header>
